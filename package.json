		"@supabase/ssr": "^0.0.10",
		"@supabase/supabase-js": "^2.39.0",
		"@sveltejs/adapter-vercel": "^3.1.0",
		"@sveltejs/kit": "^1.28.0",
		"@types/cookie": "^0.6.0",
		"@vercel/analytics": "^1.1.1",
		"@vercel/speed-insights": "^1.0.1",
		"autoprefixer": "^10.4.16",
		"bad-words": "^3.0.4",
		"daisyui": "^4.4.19",
		"eslint": "^8.55.0",
		"eslint-config-prettier": "^9.1.0",
